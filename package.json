{
  "name": "ibase-plus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:local": "vue-cli-service build && rm -rf /opt/homebrew/var/www/base2 && cp -r base2 /opt/homebrew/var/www/",
    "build:report": "vue-cli-service build --report && open base2/report.html",
    "build:report:json": "vue-cli-service build --report-json && code base2/report.json",
    "test:unit": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint",
    "register:reset": "yarn config set registry https://registry.yarnpkg.com",
    "register:update": "yarn config set registry http://ires.58corp.com/repository/58npm/",
    "reinstall": "rm -rfv node_modules && yarn",
    "start": "vue-cli-service serve",
    "compile:wmda": "yarn terser src/utils/wmda.util.js -c -m"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.5",
    "dhtmlx-gantt": "^7.1.12",
    "ee-component": "^0.1.6",
    "element-ui": "^2.15.10",
    "lodash": "^4.17.21",
    "pinia": "^2.0.14",
    "qiankun": "^2.7.3",
    "sortablejs": "^1.15.0",
    "uuid": "^8.3.2",
    "v-viewer": "^1.6.4",
    "vue": "^2.7.14",
    "vue-router": "^3.6.4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "~4.5.19",
    "@vue/test-utils": "^1.3.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^10",
    "style-resources-loader": "^1.5.0",
    "terser": "^5.16.1",
    "vue-template-compiler": "^2.7.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "prettier --write"
    ]
  }
}
